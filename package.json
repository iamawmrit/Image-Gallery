{
  "name": "gallery-desktop",
  "productName": "Gallery",
  "version": "1.0.0",
  "description": "A beautiful, fast, and private photo gallery for your desktop.",
  "type": "module",
  "main": "src/main/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamawmrit/Image-Gallery.git"
  },
  "scripts": {
    "start": "electron src/main/main.js",
    "dist": "electron-builder --config electron-builder.yml",
    "clean": "rimraf dist build",
    "postinstall": "electron-rebuild -f -w better-sqlite3"
  },
  "keywords": [
    "photo",
    "gallery",
    "electron",
    "desktop"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@electron/rebuild": "^4.0.0",
    "electron": "^40.6.0",
    "electron-builder": "^26.8.1",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "chokidar": "^4.0.3",
    "electron-store": "^10.0.0",
    "fabric": "^7.2.0",
    "sharp": "^0.34.5"
  },
  "overrides": {
    "ajv": "^8.18.0",
    "minimatch": "^10.2.1",
    "glob": "^11.0.0",
    "rimraf": "^6.0.1",
    "tar": "^7.5.8"
  }
}